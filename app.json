{
  "expo": {
    "name": "calculadora",
    "slug": "calculadora",
    "version": "1.0.0",
    "orientation": "portrait",
    // "icon": "./assets/images/icon.png", // Removed due to missing file
    "scheme": "calculadora",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      // "image": "./assets/images/splash-icon.png", // Removed due to missing file
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        // "foregroundImage": "./assets/images/adaptive-icon.png", // Removed due to missing file
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static"
      // "favicon": "./assets/images/favicon.png" // Removed due to missing file
    },
    "plugins": [
      "expo-router", //Assim que come√ßa
      [
        "expo-sqlite",
        {
          "enableFTS": true, // Ativa o suporte a Full-Text Search
          "useSQLCipher": true, // Ativa o uso do SQLCipher para criptografia de banco de dados
          "android": {
            "enableFTS": false, // Desativa o suporte a Full-Text Search no Android
            "useSQLCipher": false // Desativa o uso do SQLCipher no Android
          }
        }
      ] //Assim que acaba
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
